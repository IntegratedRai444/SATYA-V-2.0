{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src",
    "test": "vitest run",
    "test:ui": "vitest",
    "analyze": "vite build --mode analyze && vite-bundle-visualizer dist/stats.html",
    "clean": "rm -rf dist .vite",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format:check": "prettier --check src",
    "check": "npm run lint && npm run format:check && npm test",
    "clear": "rm -rf node_modules dist .vite .eslintcache .turbo package-lock.json"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "vite": "^4.0.0"
  },
  "proxy": "http://localhost:5000"
}