  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "cd client && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "start:satyaai": "node start-satyaai.js",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:client": "cd client && npm run build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "start:prod": "NODE_ENV=production pm2 start ecosystem.config.js",
    "stop:prod": "pm2 stop ecosystem.config.js",
    "restart:prod": "pm2 restart ecosystem.config.js",
    "logs:prod": "pm2 logs",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "setup": "bash scripts/setup.sh",
    "deploy": "bash scripts/deploy.sh",
    "docker:build": "docker build -t satyaai .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "test:health": "npm run health && curl -f http://localhost:5001/health || exit 1",
    "complete": "tsx server/scripts/complete-satyaai.ts",
    "validate": "tsx server/tests/run-validation.ts",
    "production-check": "tsx server/tests/production-readiness-check.ts"
  },
