{
  "name": "rest-express",
  "version": "1.1.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:node\" \"npm run dev:python\" \"npm run dev:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:node": "tsx server/index.ts",
    "dev:python": "cd server/python && python main_api.py",
    "dev:all": "npm run dev",
    "start:all": "npm run dev",
    "start:satyaai": "npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "start:prod": "NODE_ENV=production pm2 start ecosystem.config.js",
    "stop:prod": "pm2 stop ecosystem.config.js",
    "restart:prod": "pm2 restart ecosystem.config.js",
    "logs:prod": "pm2 logs",
    "check": "tsc --noEmit",
    "check:strict": "tsc --strict",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --maxWorkers=2",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPatterns=unit",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPatterns=integration",
    "test:load": "cross-env NODE_ENV=test jest --testPathPatterns='.*\\.load\\.(test|spec)\\.(ts|js)$' --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "setup": "bash scripts/setup.sh",
    "deploy": "bash scripts/deploy.sh",
    "docker:build": "docker build -t satyaai .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "test:health": "npm run health && curl -f http://localhost:5001/health || exit 1",
    "test:load:baseline": "k6 run load-tests/k6-baseline.js",
    "test:load:stress": "k6 run load-tests/k6-stress.js",
    "test:load:spike": "k6 run load-tests/k6-spike.js",
    "test:load:soak": "k6 run load-tests/k6-soak.js",
    "test:load:upload": "k6 run load-tests/k6-upload.js",
    "test:load:artillery": "artillery run load-tests/artillery-complete-flow.yml",
    "test:load:all": "npm run test:load:baseline && npm run test:load:stress && npm run test:load:spike",
    "complete": "tsx server/scripts/complete-satyaai.ts",
    "validate": "tsx server/tests/run-validation.ts",
    "production-check": "tsx server/tests/production-readiness-check.ts"
  },
